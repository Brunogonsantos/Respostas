using System;

namespace Gupy
{
    class Program
    {
        static void Main(string[] args)
        {
            int INDICE, SOMA, K;

            INDICE = 13;
            SOMA = 0;
            K = 0;

            while (K < INDICE)
            {
                K = K + 1;
                SOMA = SOMA + K;
            }


            Console.WriteLine(" O valor da SOMA é " + SOMA);
        }
    }
}
Saida:
O valor da SOMA é 91

    
